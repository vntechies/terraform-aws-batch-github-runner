{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "GitHubWebhook",
  "type": "object",
  "properties": {
    "action": { 
	  "type": "string",
	  "enum": ["queued"]
	},
	"workflow_job": {
	  "type": "object",
	  "properties": {
          "name": {
            "type": "string"		  	
          },
          "labels": {
            "type": "array"		  
          }
	  },
      "required": [ "labels" ]	  
	},
    "organization": {
	  "type": "object",
	  "properties": {
          "login": {
            "type": "string",	
            "enum": [ "tnx-journey-to-cloud" ]
          }
	  },
    "required": [ "login" ]	  
	},
	"repository": {
	  "type": "object",
	  "properties": {
          "name": {
            "type": "string"		  	
          },
          "full_name": {
            "type": "string"		  
          },
          "private": {
            "type": "boolean",
            "enum": [ true ]
          }
	  }	  
	},
	"sender": {
	  "type": "object",
	  "properties": {
          "login": {
            "type": "string",
            "enum": ${jsonencode([for user-id in org-user-ids : "${user-id}"])}
          }
	  }	  
	}
  },
  "required": [ "action", "workflow_job", "repository", "sender", "organization" ]
}
